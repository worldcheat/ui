local function notif(title, text, dur)
    pcall(function()
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = title,
            Text = text,
            Duration = dur or 5
        })
    end)
end

local id = game.PlaceId
local games = {} -- supported games

games[136801880565837] = function() -- fps flick
    loadstring(game:HttpGet("https://raw.githubusercontent.com/worldcheat/PulseGrid/refs/heads/main/Fps%20flick%20%20mobile%20%2B%20pc"))()
end

games[11177482306] = function() -- streetz wars 2
    loadstring(game:HttpGet("https://raw.githubusercontent.com/worldcheat/PulseGrid/refs/heads/main/Sw2", true))()
end

local run = games[id]

if run then
    notif("PulseGrid", "game supported")
    run()
else
    notif("PulseGrid", "game not supported", 4)
end
